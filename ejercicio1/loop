loop:
    mov3 rosa 
    circulosg1
    mov 3 celeste 
    circulos g2
    mov3 amarillo 
    circulos g3
    delay
    mov3 celeste
    circulosg1 
    mov3 amarillo 
    circulosg2 
    mov3 rosa 
    circulosg3
    delay 
    mov3 amarillo 
    circulosg1 
    mov3 rosa
    circulosg2 
    mov3 celeste 
    circulosg3 

    movz x10, 0xFF, lsl 16
    movk x10, 0xFFFF, lsl 0
    movz x3, 0xfc, lsl 16   // defino el color
    movk x3, 0x52f7, lsl 0  // termino de definir un color
    bl circulosg1
    movz x3, 0x75, lsl 16   // defino el color
    movk x3, 0xfcfc, lsl 0  // termino de definir un color
    bl circulosg2
    movz x3, 0xfb, lsl 16   // defino el color
    movk x3, 0xd65a, lsl 0  // termino de definir un color
    bl circulosg3
    bl delay
    movz x3, 0x75, lsl 16   // defino el color
    movk x3, 0xfcfc, lsl 0  // termino de definir un color
    bl circulosg1
    movz x3, 0xfb, lsl 16   // defino el color
    movk x3, 0xd65a, lsl 0  // termino de definir un color
    bl circulosg2
    movz x3, 0xfc, lsl 16   // defino el color
    movk x3, 0x52f7, lsl 0  // termino de definir un color
    bl circulosg3
    bl delay
    movz x3, 0xfb, lsl 16   // defino el color
    movk x3, 0xd65a, lsl 0  // termino de definir un color
    bl circulosg1
    movz x3, 0xfc, lsl 16   // defino el color
    movk x3, 0x52f7, lsl 0  // termino de definir un color
    bl circulosg2
    movz x3, 0xfb, lsl 16   // defino el color
    movk x3, 0xd65a, lsl 0  // termino de definir un color
